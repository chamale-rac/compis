%token ID
%token PLUS
%token TIMES
%token POWER  // Non-associative operator for causing shift-reduce conflict
%token LPAREN RPAREN
%token WS
IGNORE WS

%%

expression:
    expression PLUS term
  | term
;

term:
    term TIMES factor
  | factor
;

factor:
    factor POWER base  // Shift-reduce conflict can occur here
  | base
;

base:
    LPAREN expression RPAREN
  | ID
;
